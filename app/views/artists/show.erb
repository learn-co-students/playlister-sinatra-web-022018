<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1>Genres</h1>
    <% @artist.genres.each do |genre| %>
    <a href="/genres/<%=genre.slug%>"><%= genre.name %></a>
    <% end %>

    <h1>Songs</h1>
    <% @songs.each do |song| %>
    <a href="/songs/<%=song.slug%>"><%= song.name %></a>
    <% end %>
  </body>
</html>
